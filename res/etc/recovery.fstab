# mount point	fstype		device      device2 (redundant)             options

/sdcard		vfat	/dev/block/mmcblk1p1
/sd-ext		vfat	/dev/block/mmcblk1p2
/system		ext4	/dev/block/stl9
/cache		ext4	/dev/block/mmcblk0p1
/data		ext4	/dev/block/mmcblk0p2
/datadata   ext4    /dev/block/stl10
/boot		bml		boot
/recovery	bml		recovery
